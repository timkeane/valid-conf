{
  "name": "valid-conf",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "build": "rm -rf ./dist && babel ./**/*.js -d ./dist --ignore __tests__,coverage",
    "prestart": "yarn build",
    "start": "node dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timkeane/valid-conf.git"
  },
  "author": "Tim Keane",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timkeane/valid-conf/issues"
  },
  "homepage": "https://github.com/timkeane/valid-conf#readme",
  "dependencies": {
    "dotenv": "^6.0.0",
    "isomorphic-fetch": "^2.2.1"
  },
  "jest": {
    "verbose": true,
    "automock": false,
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "collectCoverageFrom": [
      "src/*.js"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.2",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "jest": "^23.6.0"
  }
}
